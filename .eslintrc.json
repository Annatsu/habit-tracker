{
  "extends": [
    "google",
    "plugin:react/recommended",
    "babel",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "object-curly-spacing": "off",
    "babel/no-invalid-this": 1
  },
  "env": {
    "es6": true
  }
}
